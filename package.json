{
  "name": "consul-node-sdk",
  "version": "0.1.2",
  "description": "Node.js SDK for Consul",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/m-t-a97/consul-node-sdk#readme",
  "bugs": {
    "url": "https://github.com/m-t-a97/consul-node-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-t-a97/consul-node-sdk.git"
  },
  "packageManager": "pnpm@10.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsdown",
    "dev": "tsx ./scripts/test-client.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "tsdown": "^0.12.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
